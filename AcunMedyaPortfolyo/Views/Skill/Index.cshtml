@model List<AcunMedyaPortfolyo.Models.Tbl_Skill>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Bu Bir Skill Sayfasıdır</h2>

<div class="Container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>SkillID</th>
                <th>SkillName</th>
                <th>Derece</th>
                <th>Description</th>
                <th>Güncelle</th>
                <th>Sil</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.SkillID</td>
                <td>@item.SkillName</td>
                <td>@item.Derece</td>
                <td>@item.Description</td>
                <td><a href="/Skill/UpdateSkill/@item.SkillID" class="btn btn-danger">Güncelle</a></td>
                <td><a href="/Skill/DeleteSkill/@item.SkillID" class="btn btn-dark">Sil</a></td>

            </tr>
        }

    </table>
</div>
<a href="/Skill/CreateSkill/" class="btn btn-danger">Oluştur</a>
