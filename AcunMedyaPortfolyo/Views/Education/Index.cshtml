@model List<AcunMedyaPortfolyo.Models.Tbl_Education>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Bu bir Educadion Sayfası</h2>
<div class="Container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>EducationID</th>
                <th>StartYear</th>
                <th>EndYear</th>
                <th>Name</th>
                <th>Description</th>
                <th>Section</th>
                <th>Güncelle</th>
                <th>Sil</th>
            </tr>
        </thead>
        <tbody>
        </tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EducationID</td>
                <td>@item.StartYear</td>
                <td>@item.EndYear</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.Section</td>
                <td><a href="/Education/UpdateEducation/@item.EducationID" class="btn btn-danger">Güncelle</a></td>
                <td><a href="/Education/DeleteEducation/@item.EducationID" class="btn btn-dark">Sil</a></td>

            </tr>
        }

    </table>
</div>
<a href="/Education/CreateEducation/" class="btn btn-danger">Oluştur</a>

