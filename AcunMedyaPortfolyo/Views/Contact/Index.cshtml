@model List<AcunMedyaPortfolyo.Models.Tbl_Contact>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Burası Contact Sayfası</h2>

<div class="Container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ContactID</th>
                <th>Description</th>
                <th>Adress</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Güncelle</th>
                <th>Sil</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ContactID</td>
                    <td>@item.Description</td>
                    <td>@item.Adress</td>
                    <td>@item.Email</td>
                    <td>@item.Phone</td>
                    <td><a href="/Contact/UpdateContact/@item.ContactID" class="btn btn-danger">Güncelle</a></td>
                    <td><a href="/Contact/DeleteContact/@item.ContactID" class="btn btn-dark">Sil</a></td>

                </tr>
            }




        </tbody>

    </table>
</div>
<a href="/Contact/CreateContact/" class="btn btn-danger">Oluştur</a>