@model List<AcunMedyaPortfolyo.Models.Tbl_Abaut>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
    int count = 0;
}

<h2>Burası Hakkımızda</h2>

<div class="Container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>AbautID</th>
                <th>ImageUrl</th>
                <th>Title</th>
                <th>BirthDay</th>
                <th>WebSite</th>
                <th>Phone</th>
                <th>City</th>
                <th>Age</th>
                <th>Email</th>
                <th>FreeLance</th>
                <th>Description1</th>
                <th>Description2</th>
                <th>Degree</th>
                <th>Güncelle</th>
                <th>Sil</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)

            {
                count++;

                <tr>
                    <td>@item.AbautID</td>
                    <td>@item.ImageURL</td>
                    <td>@item.Title</td>
                    <td>@item.BirthDay</td>
                    <td>@item.WebSite</td>
                    <td>@item.Phone</td>
                    <td>@item.City</td>
                    <td>@item.Age</td>
                    <td>@item.Email</td>
                    <td>@item.FreeLance</td>
                    <td>@item.Description1</td>
                    <td>@item.Description2</td>
                    <td>@item.Degree</td>
                    <td><a href="/About/UpdateAbout/@item.AbautID" class="btn btn-danger">Güncelle</a></td>
                    <td><a href="/About/DeleteAbout/@item.AbautID" class="btn btn-dark">Sil</a></td>
                </tr>

            }
        </tbody>
    </table>
    <a href="/About/CreateAbout/" class="btn btn-danger">Oluştur</a>




</div>
