@model List<AcunMedyaPortfolyo.Models.Tbl_Project>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Bu Bir Project Sayfasıdır</h2>

<div class="Container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ProjectID</th>
                <th>ProjectName</th>
                <th>Description</th>
                <th>ProjectLink</th>
                <th>Image1</th>
                <th>Image2</th>
                <th>Image3</th>
                <th>CategoryID</th>
                <th>Güncelle</th>
                <th>Sil</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProjectID</td>
                <td>@item.ProjectName</td>
                <td>@item.Description</td>
                <td>@item.ProjectLink</td>
                <td>@item.Image1</td>
                <td>@item.Image2</td>
                <td>@item.Image3</td>
                <td>@item.CategoryID</td>
                <td><a href="/Project/UpdateProject/@item.ProjectID" class="btn btn-danger">Güncelle</a></td>
                <td><a href="/Project/DeleteProject/@item.ProjectID" class="btn btn-dark">Sil</a></td>

            </tr>
        }

    </table>
</div>
<a href="/Prpject/CreateProject/" class="btn btn-danger">Oluştur</a>